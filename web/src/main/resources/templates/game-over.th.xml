<thlogic>
    <attr sel="#headerId" th:replace="~{fragments/head :: headFragment}"/>
    <attr sel="#footer" th:replace="~{fragments/footer :: footerFragment}"/>
    <attr sel="#resultMsg" th:text="${resultMessage}"/>
    <attr sel="#h1Text" th:text="#{game.title}"/>
    <attr sel="#overText" th:text="#{game.over}"/>
    <attr sel="#restartLink" th:text="#{game.playAgain}" th:href="restart"/>
    <attr sel="#homeLink" th:text="#{game.home}" th:href="'/'"/>
</thlogic>